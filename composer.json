{
    "name": "vdhsoft-com/laravel-test-package",
    "description": "A test Laravel package",
    "type": "library",
    "license": "MIT",
    "version": "1.0.0", 
    "keywords": [
        "laravel",
        "test",
        "package"
    ],
    "authors": [
        {
            "name": "VDHSoft",
            "email": "info@vdhsoft.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "php": "^8.1"
    },
    "autoload": {
        "psr-4": {
            "VDHSoft\\TestPkg\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "VDHSoft\\TestPkg\\Test\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit"
    }
